{% extends '@admin/admin_base.html.twig' %}

{% block title %}Detail!{% endblock %}

{% block body %}
<div class="maintenance-wrapper">

    {% if maintenance is not empty %}

        {% trans from 'maintenance' %}uuid{% endtrans %}: {{ maintenance.uuid }}<br/>
        {% trans from 'maintenance' %}mode{% endtrans %}: {{ maintenance.mode }}<br/>
        {% trans from 'maintenance' %}redirectPath{% endtrans %}: {{ maintenance.redirectPath }}<br/>

        {% for ip in maintenance.ips %}
            {% trans from 'maintenance' %}ip{% endtrans %}: {{ ip }}<br/>
        {% endfor %}

    <a href="{{ path('maintenance.list') }}">{% trans from 'global' %}List{% endtrans %}</a>
    <a href="{{ path('maintenance.update', {"uuid": maintenance.uuid}) }}">{% trans from 'global' %}Update{% endtrans %}</a>
    {% endif %}

</div>

 {% endblock %}
